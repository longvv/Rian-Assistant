{
  "model_list": [
    {
      "model_name": "my-claude-bot", 
      "model": "${PICOCLAW_AGENTS_DEFAULTS_MODEL}",
      "api_key": "${PICOCLAW_PROVIDERS_ANTHROPIC_API_KEY}"
    }
  ],
  "agents": {
    "defaults": {
      "provider": "${PICOCLAW_AGENTS_DEFAULTS_PROVIDER}"
    }
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "token": "${PICOCLAW_CHANNELS_TELEGRAM_TOKEN}",
      "allow_from": ["${PICOCLAW_CHANNELS_TELEGRAM_ALLOW_FROM}"]
    }
  }
}
